<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mleith785.cs499.firebasedb">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FirebaseDB">
        <activity android:name=".SearchByNameCityFeat"></activity>
        <activity android:name=".AddSiteSaveActivity" />
        <activity android:name=".AddSiteMapActivity" />
        <activity android:name=".FavoritesActivity" />
        <activity android:name=".CampsiteDetailsActivity" />
        <activity android:name=".MapListNavActivity" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="${MAPS_API_KEY}" />

        <activity
            android:name=".SearchByLocation"
            android:screenOrientation="portrait" />
        <activity
            android:name=".HomePageActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".FirebaseLoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Image_test"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Campsite_DB_Test"
            android:screenOrientation="portrait" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            android:screenOrientation="portrait"
        </activity>
    </application>

</manifest>